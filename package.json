{
  "name": "ai-chatbot",
  "version": "3.0.23",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "npm run db:migrate:safe && next build",
    "build:local": "next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:migrate:safe": "npx tsx lib/db/migrate.ts || echo 'Migration completed or already applied'",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:setup": "tsx lib/scripts/setup-db.ts",
    "db:ingest": "tsx lib/scripts/ingest.ts",
    "db:test:setup": "tsx scripts/setup-test-db.ts",
    "db:test:migrate": "tsx lib/db/test-migrate.ts",
    "db:test:seed": "tsx scripts/seed-test-db.ts",
    "db:test:reset": "tsx scripts/reset-test-db.ts",
    "db:test:validate": "tsx scripts/validate-test-db.ts",
    "db:test:clean": "tsx scripts/reset-test-db.ts && echo 'Test database cleaned'",
    "validate:providers": "tsx scripts/validate-providers.ts",
    "test": "export PLAYWRIGHT=true && bun run playwright test",
    "test:safe": "bun scripts/test-with-timeout.ts",
    "test:stagehand": "export PLAYWRIGHT=true && npx playwright test tests/stagehand/ --max-failures=1",
    "test:stagehand:headed": "export PLAYWRIGHT=true && npx playwright test tests/stagehand/ --headed",
    "test:stagehand:check": "tsx scripts/check-stagehand-env.ts",
    "test:stagehand:single": "tsx scripts/run-stagehand-test.ts",
    "test:no-stagehand": "export PLAYWRIGHT=true && npx playwright test --grep-invert stagehand",
    "test:unit": "USE_REAL_DB=true PLAYWRIGHT=false bun test tests/voice-slice4-validation.test.ts tests/signal-handlers.test.ts --timeout 10000",
    "test:unit:safe": "PLAYWRIGHT=false bun test tests/voice-slice4-validation.test.ts --timeout 5000",
    "test:e2e": "export PLAYWRIGHT=true && bun run playwright test tests/e2e/ --max-failures=1",
    "test:routes": "export PLAYWRIGHT=true && bun run playwright test tests/routes/ --max-failures=1",
    "test:all": "bun run test:unit:safe && bun run test:stagehand && bun run test:e2e",
    "test:no-skip": "./tests/run-all-tests.sh",
    "test:100": "export USE_MOCK_SERVICES=true && bun run test",
    "test:watch": "bun test --watch --timeout 10000",
    "test:tdd": "export PLAYWRIGHT=true && npx playwright test tests/stagehand/ --ui",
    "dev:mastra": "bunx mastra dev",
    "setup:mcp": "tsx scripts/setup-mcp-server.ts",
    "stagehand:demo": "tsx scripts/stagehand-demo.ts",
    "test:browserbase": "tsx scripts/test-browserbase-connection.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/cohere": "^1.2.10",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/xai": "^1.2.16",
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^2.3.1",
    "@codemirror/lang-python": "^6.2.1",
